class Stack:
   maxx="np.nan"
   def __init__(self):
     self.stack=[]

   def push(self, item):
       self.stack.append(item)
       self._update_max(item)

   def _update_max(self, item):
       if len(self.stack)==1:
           self.maxx = item
       elif item > self.maxx:
           self.maxx = item

   def pop(self):
       try:
           y=self.stack[-1]
           del self.stack[-1]
           return y
       except:
           return "no elements in stack"

   def get_max(self):
    try:
       z=self.stack[0]
       for i in self.stack:
           if i > z:
               z = i
       return z
    except:
       return "np.nan"

test_stack = Stack()

for i in range(20,-5,-1):
    test_stack.push(i)
for i in range(2,18):
    test_stack.push(i)

print(test_stack.stack)
print(test_stack.maxx)
