class Stack:
   maxx="np.nan"
   minn="np.nan"
   def __init__(self):
     self.stack=[]
     self.stack_sort=[]

   def push(self, item):
       self.stack.append(item)
       self._update_max(item)
       self._update_min(item)

   def _update_max(self, item):
       if len(self.stack)==1:
           self.maxx = item
       elif item > self.maxx:
           self.maxx = item
           self.stack_sort.append(self.maxx)

   def _update_min(self, item):
       if len(self.stack)==1:
           self.minn = item
       elif item < self.minn:
           self.minn = item
           self.stack_sort.insert(0,self.minn)

   def pop(self):
       try:
           y=self.stack[-1]
           if y==self.maxx:


           del self.stack[-1]
           return y
       except:
           return "no elements in stack"

   def get_max(self):
       return self.maxx

test_stack = Stack()

for i in range(20,-5,-1):
    test_stack.push(i)
for i in range(2,28):
    test_stack.push(i)

print(test_stack.stack)
print(test_stack.maxx)
print(test_stack.minn)
print(test_stack.stack_sort)


print("\n")
aa=[0,1,2]
aa.insert(0,113)
print(aa)
