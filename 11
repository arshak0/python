class Stack:
   def __init__(self):
      self.items = []
   def push(self, item):
         self.items.append(item)
   def pop(self):
       x=len(self.items)-1
       del self.items[x]
       return self.items[-1]

s = Stack()
s.push("hello")
s.push("true")
s.push("tru")
print(s.pop())
print(s.items)
